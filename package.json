{
  "name": "trading-master-back",
  "version": "1.0.0",
  "description": "智能加密货币交易后端系统",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:setup": "node -e \"console.log('请先安装并启动 MySQL 和 Redis 服务')\"",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "crypto",
    "trading",
    "binance",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "bull": "^4.11.0",
    "cors": "^2.8.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "mysql2": "^3.6.0",
    "p-limit": "^2.3.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "uuid": "^13.0.0",
    "ws": "^8.14.0"
  }
}
